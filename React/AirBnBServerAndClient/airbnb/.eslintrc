{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "quotes": [1, "single"],
        "strict": [1, "never"],
        "react/jsx-uses-react": 4,
        "react/jsx-uses-vars": 4,
        "react/react-in-jsx-scope": 4
    },
    "plugins": ["react"]
}
